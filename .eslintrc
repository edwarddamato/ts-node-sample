{
  "parser": "typescript-eslint-parser",
  "plugins": ["jest", "prettier"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 120,
        "singleQuote": true,
        "trailingComma": "none",
        "bracketSpacing": true,
        "parser": "typescript"
      }
    ]
  },
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  }
}
