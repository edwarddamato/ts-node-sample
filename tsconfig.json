{
  "compilerOptions": {
    "outDir": "ts-server-build",
    "target": "es5",
    "lib": [ "es2015" ],
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,                /* Report errors on unused locals. */
    "noUnusedParameters": true,            /* Report errors on unused parameters. */
    "baseUrl": "src"
  }
}
